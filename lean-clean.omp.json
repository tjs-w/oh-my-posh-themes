{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "base": "#1e1d2d",
    "blue": "#89b4fa",
    "cyan": "#89dceb",
    "indigo": "#cdd6f4",
    "crust": "#11111b",
    "flamingo": "#f2cdcd",
    "green": "#a6e3a1",
    "orange": "#fb923c",
    "lavender": "#b4befe",
    "maroon": "#eba0ac",
    "mauve": "#cba6f7",
    "peach": "#fab387",
    "pink": "#f5c2e7",
    "red": "#f38ba8",
    "rosewater": "#f5e0dc",
    "sky": "#89dceb",
    "subtext1": "#bac2de",
    "surface0": "#313244",
    "surface1": "#45475a",
    "teal": "#94e2d5",
    "text": "#cdd6f4",
    "yellow": "#f9e2af"
  },
  "secondary_prompt": {
    "template": "❯ ",
    "foreground": "p:sky"
  },
  "transient_prompt": {
    "template": "❯ ",
    "foreground": "p:yellow",
    "foreground_templates": [
      "{{ if .Root }}p:red{{ end }}"
    ]
  },
  "blocks": [
    {
      "type": "rprompt",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context",
            "fetch_version": true,
            "fetch_virtual_env": true
          },
          "template": "{{ if .Venv }}⚡{{ .Venv }} {{ end }}{{ if .Full }}{{ .Full }}{{ end }}",
          "foreground": "p:flamingo",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context",
            "fetch_version": true
          },
          "template": "{{ if .Version }} Go {{ .Version }}{{ end }}",
          "foreground": "p:cyan",
          "type": "go",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context",
            "fetch_version": true
          },
          "template": "{{ if .Version }} {{ .Version }}{{ end }}",
          "foreground": "p:mauve",
          "type": "rust",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": "{{ if .Version }} ⬢ {{ .Version }}{{ end }}",
          "foreground": "p:green",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if gt .Code 0 }} !{{ .Code }}{{ end }}",
          "foreground": "p:red",
          "type": "exit",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .SSHSession }}{{ .HostName }} {{ end }}",
          "foreground": "p:orange",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "",
            "home_icon": "",
            "style": "agnoster_short"
          },
          "template": "{{ .Path }}",
          "foreground": "p:blue",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "template": "{{ if .RepoName }} • {{ .UpstreamIcon }} {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} △{{ .Working.Modified }}{{ end }}{{ if .Staging.Changed }} ▲{{ .Staging.Modified }}{{ end }}{{ end }}",
          "foreground": "p:green",
          "foreground_templates": [
            "{{ if .Staging.Changed }}p:mauve{{ else if .Working.Changed }}p:peach{{ else }}p:green{{ end }}"
          ],
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ❯",
          "foreground": "p:yellow",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Root }}p:red{{ end }}"
          ]
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
